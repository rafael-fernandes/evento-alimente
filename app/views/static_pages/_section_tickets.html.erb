            <div class="category">BÁSICO 1</div>
            <div class="bar mt-1"></div>
            <div class="description pt-4 mt-2">
              Welcome kit<br>Platéia <strong>GOLD 1</strong>
            </div>
          </div>
        
          <div class="buy">
            <div class="price -taxa" data-toggle="tooltip" data-placement="top" title="Taxa de serviço da plataforma de e-commerce equivalente a 10% do valor do ingresso.">
              R$ 800 + TAXA
              <%= fa_icon "question-circle-o" %>
            </div>
            <a href="https://www.sympla.com.br/1-congresso-internacional-low-carb__258690" target="_blank">
              <div class="buy-ticket">COMPRAR</div>
            </a>
          </div>
        </div>
  
        <div class="col-md-3 col-lg-2 p-0 box -pre-venda">
